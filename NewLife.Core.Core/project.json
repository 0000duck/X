        "../NewLife.Core/IO/*.cs",
        "../NewLife.Core/Log/*.cs",
        "../NewLife.Core/Messaging/*.cs",
        "../NewLife.Core/Model/*.cs",
        "../NewLife.Core/Net/*.cs",
        "../NewLife.Core/Reflection/*.cs",
        "../NewLife.Core/Remoting/*.cs",
        "../NewLife.Core/Security/*.cs",
        "../NewLife.Core/Serialization/*.cs",
        "../NewLife.Core/Threading/*.cs",
        "../NewLife.Core/Web/*.cs",
        "../NewLife.Core/Xml/*.cs",
        "../NewLife.Core/Setting.cs"
      ],
      "exclude": [
        "../NewLife.Core/Common/CombGuid.cs",
        "../NewLife.Core/Common/HardInfo.cs",
        "../NewLife.Core/Log/TextControlLog.cs"
      ]
    }
  }
