{
  "version": "1.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Http.Abstractions": "1.0.0-rc2-final",
    "Microsoft.Win32.Registry": "4.0.0-rc2-24027",
    "NETStandard.Library": "1.6.0",
    "System.Collections.Specialized": "4.0.1-rc2-24027",
    "System.Diagnostics.Process": "4.1.0-rc2-24027",
    "System.Runtime.Serialization.Primitives": "4.1.1-rc2-24027",
    "System.Threading.Thread": "4.0.0-rc2-24027",
    "System.Threading.ThreadPool": "4.0.10-rc2-24027",
    "System.Xml.XmlDocument": "4.0.1-rc2-24027",
    "System.Xml.XmlSerializer": "4.0.11-rc2-24027"
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    }
  },
  "buildOptions": {
    "xmlDoc": true,
    "defines": [ "__CORE__" ],
