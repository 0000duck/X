                            <nbc:NavListItem Text="Agenda" />
                            <nbc:NavListItem Header="true" Text="Votações" Icon="road" />
                            <nbc:NavListItem Text="Perguntas" />
                            <nbc:NavListItem Text="Respostas" />
                            <nbc:NavListItem Header="true" Text="Executivos" Icon="user" />
                            <nbc:NavListItem Text="Executivos" />
                            <nbc:NavListItem Text="Partidos" />
                            <nbc:NavListItem Text="Cargos" />
